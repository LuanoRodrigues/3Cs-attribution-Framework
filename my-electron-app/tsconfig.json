{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "sourceMap": true,
    "lib": [
      "ES2022",
      "DOM"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/windows/settingsRenderer.ts"
  ]
}
