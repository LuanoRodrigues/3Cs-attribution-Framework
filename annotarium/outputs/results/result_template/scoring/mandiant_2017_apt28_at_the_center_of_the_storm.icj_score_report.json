    "claims": [
      {
        "claim_id": "C001",
        "claim_kind": "intrusion_set_attribution",
        "claim_text": "RUSSIA STRATEGICALLY EVOLVES ITS CYBER OPERATIONS",
        "actor": {
          "name": "APT1",
          "level": "None"
        },
        "object": {
          "operation": "intrusion_set",
          "time_start": "",
          "time_end": ""
